{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.options": {
       "configFile": "/Users/АлексКейт/Desktop/Кексобукинг/.eslintrc.json"
    }

}
